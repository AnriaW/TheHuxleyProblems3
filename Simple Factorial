#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int fatorial(int n){
    if(n == 1 || n == 0){
     return 1;   
    }
    
    else{
    return n*fatorial(n-1);
    }

}

int main() {
    
    int n;
    scanf ("%d", &n);
    int x = fatorial(n);
    printf("%d\n", x);
    
}
